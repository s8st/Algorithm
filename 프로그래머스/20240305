using System;

public class Solution {
    public int[] solution(int money) {
        //int[] answer = new int[] {};
        int[] answer = new int[2];
        
        // [0] : 아메리카노 잔 수 == quotient
        // [1] : 남은 돈  == 나머지 == remainder
        // 가진 돈 : money
        // money ÷ 5500 = quotient ...remainder
        
        answer[0] = money / 5500;
        answer[1] = money % 5500; 
        
        
        
        return answer;
    }
}
